      <name />
    </author>
    <link rel="edit" title="Session" href="Sessions(100295)" />
    <link rel="http://schemas.microsoft.com/ado/2007/08/dataservices/related/PASS_OData_Orator_Event" type="application/atom+xml;type=entry" title="PASS_OData_Orator_Event" href="Sessions(100295)/PASS_OData_Orator_Event" />
