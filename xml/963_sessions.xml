        <d:SessionTrack>Application &amp; Database Development</d:SessionTrack>
        <d:SessionCode></d:SessionCode>
        <d:SessionRoom>P315</d:SessionRoom>
        <d:SessionPrerequisites>Intermediate to advanced understanding of database theory and practical skills.</d:SessionPrerequisites>
        <d:SessionLevel>Intermediate</d:SessionLevel>
        <d:SessionURL>https://www.sqlsaturday.com/837/Sessions/Details.aspx?sid=87366</d:SessionURL>
