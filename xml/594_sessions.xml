        <d:SessionTrack>Enterprise Database Administration &amp; Deployment</d:SessionTrack>
        <d:SessionCode></d:SessionCode>
        <d:SessionRoom>ClassroomB</d:SessionRoom>
        <d:SessionPrerequisites>Understanding of TSQL and database file structures.</d:SessionPrerequisites>
        <d:SessionLevel>Beginner</d:SessionLevel>
        <d:SessionURL>https://www.sqlsaturday.com/528/Sessions/Details.aspx?sid=46721</d:SessionURL>
