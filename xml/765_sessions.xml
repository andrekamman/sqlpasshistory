        <d:SessionTrack>Developers</d:SessionTrack>
        <d:SessionCode></d:SessionCode>
        <d:SessionRoom>Excel</d:SessionRoom>
        <d:SessionPrerequisites>Attendees must have a passing knowledge of developing with T-SQL.</d:SessionPrerequisites>
        <d:SessionLevel>Intermediate</d:SessionLevel>
