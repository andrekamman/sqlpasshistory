        <d:SessionPrerequisites></d:SessionPrerequisites>
        <d:SessionLevel>Advanced</d:SessionLevel>
        <d:SessionURL>https://www.sqlsaturday.com/870/Sessions/Details.aspx?sid=95659</d:SessionURL>
