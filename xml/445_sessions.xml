    <id>http://feeds.pass.org/public/events/OratorDataService.svc/Sessions(12912)</id>
    <title type="text"></title>
    <updated>2020-12-28T21:44:45Z</updated>
    <author>
      <name />
    </author>
    <link rel="edit" title="Session" href="Sessions(12912)" />
    <link rel="http://schemas.microsoft.com/ado/2007/08/dataservices/related/PASS_OData_Orator_Event" type="application/atom+xml;type=entry" title="PASS_OData_Orator_Event" href="Sessions(12912)/PASS_OData_Orator_Event" />
