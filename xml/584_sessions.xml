    <title type="text"></title>
    <updated>2020-12-28T21:45:13Z</updated>
    <author>
      <name />
    </author>
    <link rel="edit" title="Session" href="Sessions(45613)" />
    <link rel="http://schemas.microsoft.com/ado/2007/08/dataservices/related/PASS_OData_Orator_Event" type="application/atom+xml;type=entry" title="PASS_OData_Orator_Event" href="Sessions(45613)/PASS_OData_Orator_Event" />
