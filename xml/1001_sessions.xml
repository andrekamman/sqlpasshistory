        <d:SessionRoom>Blue room</d:SessionRoom>
        <d:SessionPrerequisites></d:SessionPrerequisites>
        <d:SessionLevel>Intermediate</d:SessionLevel>
        <d:SessionURL>https://www.sqlsaturday.com/868/Sessions/Details.aspx?sid=94813</d:SessionURL>
