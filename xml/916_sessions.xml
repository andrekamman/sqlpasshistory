        <d:SessionTrack>BI Platform Architecture, Development &amp; Administration</d:SessionTrack>
        <d:SessionCode></d:SessionCode>
        <d:SessionRoom>Auditorium 2</d:SessionRoom>
        <d:SessionPrerequisites>Basic Knowledge of the graph theory.</d:SessionPrerequisites>
        <d:SessionLevel>Advanced</d:SessionLevel>
